{
  "expo": {
    "name": "FFApp",
    "slug": "ff-app",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "ffapp",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.manishankarvakta.FFApp"
    },
    "android": {
  "adaptiveIcon": {
    "foregroundImage": "./assets/images/icon.png",
    "backgroundColor": "#000000"
  },
  "edgeToEdgeEnabled": false,
  "package": "com.manishankarvakta.FFApp",
  "blockedPermissions": [
    "com.google.android.gms.permission.AD_ID"
  ],
  "versionCode": 4,
  "navigationBar": {
    "visible": true,
    "backgroundColor": "#000000"
  }
},
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon-dark.png",
          "imageWidth": 800,
          "resizeMode": "contain",
          "backgroundColor": "#000000"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "9a215206-00f1-44d7-85c4-35973e56a0e3"
      }
    },
    "owner": "manishankarvakta"
  }
}
